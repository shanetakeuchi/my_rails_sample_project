<% provide(:title, 'New Note') %> <h1>New Note</h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_with(model: @user, local: true) do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title, class: 'form-control' %>
        <%= f.label :info %>
        <%= f.text_field :info, class: 'form-control' %>
        <%= f.label :level %>
        <%= f.text_field :level, class: 'form-control'%>
        <%= f.submit "Create new note", class: "btn btn- primary" %>
      <% end %>
    </div>
</div>